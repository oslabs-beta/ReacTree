{
  "name": "reactree",
  "displayName": "REACTREE",
  "description": "React higherarchy visualizer and component preview tool",
  "version": "0.0.1",
  "icon": "icon.png",
  "publisher": "REACTREE",
  "preview": true,
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "note",
    "vscode"
  ],
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reacTree.start",
        "title": "ReacTree: Show Panel"
      }
    ]
  },
  "extensionPack": [
    "docsmsft.docs-markdown",
    "docsmsft.docs-preview"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config webpack.config.ts",
    "watch": "webpack --mode development --config webpack.config.ts --watch --info-verbosity verbose",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/dagre": "^0.7.48",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.20.55",
    "@types/object-path": "^0.11.0",
    "@types/react": "^16.14.35",
    "@types/react-dom": "^16.9.4",
    "@types/rimraf": "^2.0.3",
    "@types/webpack": "^4.41.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.5.4",
    "typescript": "^4.9.5",
    "vscode": "^1.1.36",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@coreui/icons": "^3.0.0",
		"@coreui/icons-react": "^2.1.0",
    "dagre": "^0.8.5",
    "fs-extra": "^8.1.0",
    "object-path": "^0.11.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "reactflow": "^11.5.2"
  }
}
